{
  "name": "babylon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run scripts/pre-dev.ts && concurrently -n \"next,daemon\" -c \"cyan,magenta\" \"next dev\" \"bun run daemon\"",
    "dev:skip-check": "concurrently -n \"next,daemon\" -c \"cyan,magenta\" \"next dev\" \"bun run daemon\"",
    "dev:next-only": "bun run scripts/pre-dev.ts && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "bun test tests/integration/ && playwright test tests/e2e && playwright test --config=synpress.config.ts",
    "test:unit": "bun test tests/integration/",
    "test:e2e": "playwright test tests/e2e",
    "test:synpress": "playwright test --config=synpress.config.ts",
    "test:headed": "playwright test --config=synpress.config.ts --headed",
    "test:ui": "playwright test --config=synpress.config.ts --ui",
    "test:verify-login": "playwright test --config=synpress.config.ts tests/synpress/00-verify-privy-login.spec.ts --headed",
    "test:critical": "playwright test --config=synpress.config.ts tests/synpress/00-critical-path.spec.ts",
    "test:complete": "playwright test --config=synpress.config.ts tests/synpress/10-complete-validation.spec.ts",
    "test:full": "playwright test --config=synpress.config.ts tests/synpress/00-critical-path.spec.ts tests/synpress/01-auth.spec.ts tests/synpress/02-navigation.spec.ts tests/synpress/03-feed-posts.spec.ts tests/synpress/05-markets.spec.ts tests/synpress/08-other-pages.spec.ts tests/synpress/10-complete-validation.spec.ts",
    "test:ci": "bun test tests/integration/ && playwright test tests/e2e",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "bun run prisma/seed.ts",
    "contracts:compile": "forge build",
    "contracts:test": "forge test",
    "contracts:deploy": "forge script scripts/DeployBabylon.s.sol:DeployBabylon --rpc-url $RPC_URL --broadcast --verify",
    "game:init": "bun run scripts/init-game-content.ts",
    "daemon": "bun run src/cli/realtime-daemon.ts"
  },
