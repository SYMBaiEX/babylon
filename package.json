{
  "name": "babylon",
  "version": "1.0.0",
  "description": "Babylon prediction market game",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "generate": "bun run src/cli/generate-game.ts",
    "generate:verbose": "bun run src/cli/generate-game.ts --verbose",
    "generate:images": "bun run src/cli/generate-actor-images.ts",
    "dev": "cd src/viewer && vite",
    "validate": "bun run src/cli/validate-actors.ts"
  },
  "dependencies": {
    "@fal-ai/client": "^1.7.0",
    "dotenv": "^17.2.3",
    "openai": "^6.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.3.3",
    "vite": "^5.4.1"
  },
  "keywords": [
    "prediction-market",
    "game-engine",
    "autonomous",
    "simulation",
    "vibevm"
  ]
}
