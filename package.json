{
  "name": "babylon",
  "version": "1.0.0",
  "description": "Babylon prediction market game",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run scripts/pre-dev.ts && concurrently --names \"WEB,GAME\" --prefix-colors \"cyan,magenta\" \"next dev\" \"tsx src/cli/realtime-daemon.ts\"",
    "dev:web-only": "bun run scripts/pre-dev.ts && next dev",
    "dev:cron-mode": "concurrently --names \"WEB,CRON\" --prefix-colors \"cyan,yellow\" \"bun run dev:web-only\" \"bun run cron:local\"",
    "dev:skip-check": "next dev",
    "postinstall": "prisma generate",
    "prebuild": "tsx scripts/bundle-prompts.ts",
    "build": "next build",
    "start": "next start",
    "build:prompts": "tsx scripts/bundle-prompts.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:integration": "bun test tests/integration",
    "test:referrals": "playwright test tests/e2e/referrals",
    "db:start": "bun run scripts/db.ts start",
    "db:stop": "bun run scripts/db.ts stop",
    "db:restart": "bun run scripts/db.ts restart",
    "db:status": "bun run scripts/db.ts status",
    "db:migrate": "bun run scripts/db.ts migrate",
    "db:seed": "bun run scripts/db.ts seed",
    "db:reset": "bun run scripts/db.ts reset",
    "storage:init": "bun run scripts/init-storage.ts",
    "daemon": "tsx src/cli/realtime-daemon.ts",
    "daemon:verbose": "tsx src/cli/realtime-daemon.ts --verbose",
    "cron:local": "bun run scripts/local-cron-simulator.ts",
    "cron:test": "curl -X POST http://localhost:3000/api/cron/game-tick -H \"Authorization: Bearer ${CRON_SECRET:-development}\"",
    "cron:trigger": "curl -X POST ${VERCEL_URL}/api/cron/game-tick -H \"Authorization: Bearer $CRON_SECRET\"",
    "status": "bun run scripts/check-game-status.ts",
    "test:feed": "bun run scripts/test-feed.ts",
    "backfill:tags": "tsx scripts/backfill-tags.ts",
    "trending:populate": "bun run scripts/populate-trending-manual.ts",
    "trending:check": "bun run scripts/check-and-populate-trending.ts",
    "fix": "bun run scripts/quick-fix.ts",
    "bootstrap": "bun run scripts/bootstrap-content.ts",
    "game:init": "bun run scripts/init-game-content.ts",
    "agents": "bun run src/cli/autonomous-agents-demo.ts",
    "agents:many": "bun run src/cli/autonomous-agents-demo.ts --count 5",
    "eliza": "tsx src/eliza/agents/run-eliza-agent.ts",
    "eliza:alice": "tsx src/eliza/agents/run-eliza-agent.ts",
    "eliza:auto": "tsx src/eliza/agents/run-eliza-agent.ts --auto-trade",
    "eliza:all": "bun run scripts/run-all-agents.ts",
    "eliza:all:auto": "bun run scripts/run-all-agents.ts --auto-trade",
    "generate": "bun run src/cli/generate-day.ts",
    "generate:init": "bun run src/cli/generate-day.ts --init",
    "generate:week": "bun run src/cli/generate-day.ts --days 7",
    "generate:month": "bun run src/cli/generate-game.ts",
    "generate:verbose": "bun run src/cli/generate-game.ts --verbose",
    "generate:images": "bun run src/cli/generate-actor-images.ts",
    "generate:user-assets": "tsx scripts/generate-user-assets.ts",
    "validate": "bun run src/cli/validate-actors.ts",
    "pools:init": "tsx scripts/init-pools.ts",
    "pools:test": "tsx scripts/test-pools-system.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "bun run prisma/seed.ts",
    "init:all": "bun run prisma/seed.ts && echo 'âœ… Database initialized with actors, pools, and leaderboard'",
    "contracts:compile": "forge build",
    "contracts:test": "forge test",
    "contracts:deploy:testnet": "bun run scripts/deploy-contracts.ts",
    "contracts:deploy:mainnet": "NETWORK=base bun run scripts/deploy-contracts.ts",
    "contracts:verify": "bash scripts/verify-contracts.sh",
    "contracts:test-reads": "bun run scripts/test-contract-reads.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/lib-storage": "^3.922.0",
    "@elizaos/core": "^1.6.3",
    "@elizaos/plugin-bootstrap": "^1.6.3",
    "@elizaos/plugin-sql": "^1.6.3",
    "@fal-ai/client": "^1.7.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@privy-io/react-auth": "3.0.1",
    "@privy-io/server-auth": "^1.32.5",
    "@privy-io/wagmi": "^2.0.2",
    "@tanstack/react-query": "5.90.5",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/ws": "^8.18.1",
    "@vercel/blob": "^2.0.0",
    "agent0-sdk": "^0.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "framer-motion": "12.23.24",
    "geist": "^1.5.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.1",
    "lucide-react": "^0.548.0",
    "multiformats": "^13.4.1",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "openai": "^6.6.0",
    "prisma": "6.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.0.5",
    "viem": "2.38.4",
    "wagmi": "2.19.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@chainlink/contracts": "^1.5.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/bun": "latest",
    "@types/node": "^24.9.1",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.4.1"
  },
  "keywords": [
    "prediction-market",
    "game-engine",
    "autonomous",
    "simulation",
    "vibevm",
    "eliza",
    "eliza-plugin",
    "ai-agent",
    "trading-bot"
  ],
  "resolutions": {
    "@eth-optimism/contracts": "npm:@eth-optimism/contracts@0.6.0",
    "ethers": "6.15.0",
    "zod": "4.1.12",
    "lucide-react": "0.548.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "trustedDependencies": [
    "@arbitrum/nitro-contracts",
    "unrs-resolver"
  ]
}
